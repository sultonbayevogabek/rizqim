        <% if (user) { %>
            <a class="user" href="/<%= user.username %>" title="Shaxsiy profilga kirish">
                <span><%= user.firstName + ' ' + user.lastName %></span>
                <div class="user-img">
                    <picture>
                        <source srcset="/img/users/user@1x.webp 1x, /img/users/user@2x.webp 2x" type="image/webp">
                        <img srcset="/img/users/user@1x.jpg 1x, /img/users/user@2x.jpg 2x">
                    </picture>
                </div>
            </a>
        <% } else { %>
            <a class="signin" href="/signin" title="Tizimga kirish">
                <span>Kirish</span>
                <img src="/img/icons/signin.svg" alt="">
