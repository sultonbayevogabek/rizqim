                <a class="nav__link <%= path === '/contact' ? 'active' : null %>" href="/contact">Aloqa</a>
            </li>
            <% if (user) { %>
                <li class="nav__li nav__li--mobile">
                    <a class="nav__link" href="/<%= user.username %>"><%= user.firstName + ' ' + user.lastName %></a>
